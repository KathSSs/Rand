#include <iostream>
#include <stdio.h>
#include <stdlib.h>
#include <string>
using namespace std;

int main() {
	for (int i = 1; i <= 20; i++) {
		cout <<1 + (rand() % 6) << "   ";
		if (i % 5 == 0) { // esta parte del codigo hace que cambie de linea si i == 5
			cout << "\n";
		}
	}

	system("pause"); 
	return 0;
  }
